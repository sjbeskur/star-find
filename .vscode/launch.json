  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'starrynight'",
      "cargo": {
        "args": ["test", "--no-run", "--lib", "--package=starrynight"],
        "filter": {
          "name": "starrynight",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'starrynight'",
      "cargo": {
        "args": ["build", "--bin=starrynight", "--package=starrynight"],
        "filter": {
          "name": "starrynight",
          "kind": "bin"
        }
      },
      "args": ["img/nonsense.png"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'starrynight'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=starrynight",
          "--package=starrynight"
        ],
        "filter": {
          "name": "starrynight",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug integration test 'app'",
      "cargo": {
        "args": ["test", "--no-run", "--test=app", "--package=starrynight"],
        "filter": {
          "name": "app",
          "kind": "test"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
